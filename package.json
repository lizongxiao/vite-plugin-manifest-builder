{
  "name": "vite-plugin-manifest-builder",
  "version": "1.0.5",
  "description": "A Vite plugin for dynamically building and modifying Chrome Extension manifest.json with CSS injection and field overrides",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "test": "node test-quick.js",
    "test:full": "node test-all-features.js",
    "test:version": "node test/version-increment.test.js",
    "test:integration": "node test/integration-test.js",
    "test:manual": "node test/manual-test.js",
    "test:persistence": "node test/version-persistence-test.js",
    "test:all": "npm run test:version && npm run test:integration && npm run test:manual && npm run test:persistence"
  },
  "keywords": [
    "vite",
    "plugin",
    "chrome-extension",
    "manifest",
    "css",
    "inject",
    "build",
    "builder",
    "transformer",
    "dynamic"
  ],
  "author": {
    "name": "lizongxiao",
    "email": "lizongxiao@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lizongxiao/vite-plugin-manifest-builder.git"
  },
  "bugs": {
    "url": "https://github.com/lizongxiao/vite-plugin-manifest-builder/issues"
  },
  "homepage": "https://github.com/lizongxiao",
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
