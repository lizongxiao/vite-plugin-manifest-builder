{
  "manifest_version": 3,
  "name": "动态配置的扩展名称",
  "version": "1.0.4",
  "description": "这是一个通过构建时动态修改的扩展描述",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-scripts.js"
      ],
      "css": [
        "css/main.css"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Example Extension"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}